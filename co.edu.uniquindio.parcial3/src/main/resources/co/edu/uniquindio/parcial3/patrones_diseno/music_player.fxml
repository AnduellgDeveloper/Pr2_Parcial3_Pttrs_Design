<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="20" alignment="CENTER" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
      fx:controller="co.edu.uniquindio.parcial3.patrones_diseno.Adapter.MusicPlayerController">
    <HBox spacing="10" alignment="CENTER">
        <Button fx:id="playButton" text="Play"/>
        <Button fx:id="pauseButton" text="Pause"/>
        <Button fx:id="stopButton" text="Stop"/>
    </HBox>
</VBox>
